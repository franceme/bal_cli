{
	"python.defaultInterpreterPath":"/resources/Python/python",
	"todo-tree.regex.regex": "(\\\\|%)\\s*($TAGS)",
	"todo-tree.general.tags": [
		"BUG",
		"HACK",
		"FIXME",
		"TODO",
		"XXX",
		"[ ]",
		"[x]",
		":>TODO",
		"note"
	],
	"github.copilot.enable": {
		"*": true,
		"yaml": false,
		"plaintext": false,
		"markdown": false
	},
	"l13Diff.confirmCopy": false,
	"editor.wordWrap": "on",
	"workbench.colorTheme": "Default Light+",
	"editor.minimap.enabled": false,
	"latex-workshop.message.error.show": false,
	"latex-workshop.message.warning.show": false,
	"telemetry.enableTelemetry": false,
	"telemetry.enableCrashReporter": false,
	"telemetry.telemetryLevel":"off",
	"workbench.editorAssociations": {
		"*.ipynb": "jupyter-notebook"
	},
	"workbench.editorAssociations": {
		"*.pdf": "pdf.preview"
	},
	"pdf-preview.default.sidebar": true,
	"git.enableSmartCommit": true,
	"git.confirmSync": false,
	"editor.wordWrap": "on",
	"latex-workshop.latex.recipes": [
		{
			"name": "tectonic",
			"tools": [
				"tectonic"
			]
		}
	],
	"files.exclude": {
		".idea": true,
		"__pycache__": true,
		"**/.ipynb_checkpoints/": true,
		"**/CachedExtensions/": true,
		"**/CachedExtensionVSIXs/": true,
		"**/extensions/": true,
		"**/Machine": true,
		"**/logs": true,
		"**/User": true,
		"**/target": true,
		"Cargo.lock": true,
		"**/machineid": true,
		"**/coder.json": true,
		"languagepacks.json": true,
		"**/.pytest_cache/": true,
		".devcontainer":true,
		".github":true,
		".gitignore":true,
		".gitattributes":true,
		"README.md":true,
		".vscode/":true,
		"**/__pycache__":true,
		"**/.fleet":true,
		"**/.nox":true,
		"**/.run":true,
		"**/appmap.yml":true,
		"OutputFormats/":true,
		"**/.vscode/":true,
		"**/CachedProfilesData/":true,
		"**/code-server-ipc.sock":true,
		"**/serve-web-key-half":true
	},
		"python.testing.unittestArgs": [
			"-v",
			"-s",
			".",
			"-p",
			"*_test.py"
		],
		"python.testing.pytestEnabled": false,
		"pythonTestExplorer.testFramework": "unittest",
		"python.testing.unittestEnabled": true,
		"python.testing.cwd": "./tests",
		"python.analysis.extraPaths": [
			"./src"
		]
}